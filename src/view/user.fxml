<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="611.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserController">
   <children>
      <Label fx:id="Username" layoutX="178.0" layoutY="14.0" text="User's Albums">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="logOutBtn" layoutX="414.0" layoutY="15.0" mnemonicParsing="false" onAction="#onClickLogout" text="Log out" />
      <Button fx:id="albumDeleteBtn" layoutX="14.0" layoutY="252.0" mnemonicParsing="false" onAction="#onClickDelete" text="Delete album" />
      <TextField fx:id="albumFld" layoutX="97.0" layoutY="45.0" prefHeight="25.0" prefWidth="250.0" />
      <Button fx:id="albumRenameBtn" layoutX="240.0" layoutY="78.0" mnemonicParsing="false" onAction="#onClickRename" text="Rename Album" />
      <Button fx:id="albumAddBtn" layoutX="148.0" layoutY="78.0" mnemonicParsing="false" onAction="#onClickAdd" text="Add Album" />
      <Button fx:id="albumOpenBtn" layoutX="13.0" layoutY="212.0" mnemonicParsing="false" onAction="#onClickOpen" prefHeight="26.0" prefWidth="90.0" text="Open Album" />
      <Label layoutX="12.0" layoutY="49.0" text="Album name:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="photosSearchBtn" layoutX="14.0" layoutY="292.0" mnemonicParsing="false" onAction="#onClickSearch" prefHeight="26.0" prefWidth="100.0" text="Search Photos" />
      <ListView fx:id="albums" layoutX="147.0" layoutY="115.0" prefHeight="483.0" prefWidth="319.0" />
   </children>
</AnchorPane>
