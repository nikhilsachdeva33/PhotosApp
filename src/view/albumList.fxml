      <Button layoutX="41.0" layoutY="363.0" mnemonicParsing="false" onAction="#onClickOpen" text="Open Photo" />
      <ChoiceBox fx:id="albumNameFld" layoutX="98.0" layoutY="48.0" prefHeight="26.0" prefWidth="352.0" />
      <ListView fx:id="photos" layoutX="164.0" layoutY="189.0" prefHeight="373.0" prefWidth="300.0" />
